{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "light-inventory",
  "version": "0.1.0",
  "identifier": "anatolii.k",
  "build": {
    "frontendDist": "../dist/light-inventory/browser",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "ng serve",
    "beforeBuildCommand": "ng build"
  },
  "app": {
    "windows": [
      {
        "title": "light-inventory",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
